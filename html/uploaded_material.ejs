<!DOCTYPE html>
<html>
<head>
	<title>Uploaded Materials</title>
    <link rel="stylesheet" href="/css/uploaded.css">
    <link rel="icon" href="/images/logo.jpg" type="image/x-icon">
</head>
<body>
    <nav>
		<div id="mySidenav" class="sidenav">
            <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
            <a href="/"><i class="fa fa-fw fa-home"> </i>Home</a>
            <a href="/Dashboard"><i class="fa fa-fw fa-search"></i>Dashboard</a>
            <a href="/Resources"><i class="fa fa-fw fa-envelope"></i>Resources</a>
            <a href="/Profile"><i class="fa fa-fw fa-user"></i>Profile</a>
          </div>
          
          <div class="container" onclick="openNav()">
            <div class="bar1"></div>
            <div class="bar2"></div>
            <div class="bar3"></div>
			<p>MENU</p>
          </div>
	  </nav>
	<h1>Your Uploaded Study Materials</h1>
	<ul>
    <% pdfs.forEach((pdf) => { %>
      <li><a href="/pdf/<%= pdf._id %>"><%= pdf.filename %></a></li>
    <% }) %>
      <li><a href="/Upload">Upload New</a></li>
  </ul>
    <script>
        function openNav() {
            document.getElementById("mySidenav").style.width = "250px";
          }
          
          /* Set the width of the side navigation to 0 */
          function closeNav() {
            document.getElementById("mySidenav").style.width = "0";
          }
          
          function myFunction(x) {
              x.classList.toggle("change");
        }
    </script>
</body>
</html>
