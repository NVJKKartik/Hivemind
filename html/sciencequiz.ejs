<!DOCTYPE html>
<html>
  <head>
    <title>Science Quiz - HiveMind</title>
    <link rel="stylesheet" href="/css/sciencequiz.css">
  </head>
  <body>
    <header>
      <nav>
        <ul>
          <li><a href="/">Home</a></li>
          <li><a href="/Dashboard">Dashboard</a></li>
          <li><a href="/Profile">Profile</a></li>
          <li><a href="/Resources">Resources</a></li>
          <li><a href="/Settings">Settings</a></li>
        </ul>
      </nav>
    </header>
    
    <main>
      <h1><%= Subject %> Quiz</h1>
      <form id="quiz-form">
        <% questions.forEach((q, i) => { %>
          <h3><%= q.text %></h3>
          <ul>
            <% q.choices.forEach((c, j) => { %>
              <li><input type="radio" name="q<%= i %>" value="<%= j %>"> <%= c %></li>
            <% }); %>
          </ul>
        <% }); %>
        <button id="submit">Submit</button>
            <input type="submit" value="Submit Quiz" class="submit-btn">
            </form>
    </main>
        <footer>
              <p>&copy; 2023 HiveMind</p>
        </footer>
        <script src="/javascript/sciencequiz.js"></script>
    </body>
</html>
